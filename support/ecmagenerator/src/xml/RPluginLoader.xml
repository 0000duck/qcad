<?xml version="1.0"?>
<unit xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:rs="http://www.ribbonsoft.com">
  <class name="RPluginLoader"
  xsi:noNamespaceSchemaLocation="../class.xsd" isCopyable="false"
  hasShell="false" sharedPointerSupport="false" isQObject="false"
  hasStreamOperator="false" isAbstract="false" isScriptable="true">
    <method name="getPluginFiles" cppName="getPluginFiles"
    specifier="public" isStatic="true" isVirtual="false"
    isScriptOverwritable="true">
      <variant returnType="QStringList" isPureVirtual="false" />
    </method>
    <method name="loadPlugins" cppName="loadPlugins"
    specifier="public" isStatic="true" isVirtual="false"
    isScriptOverwritable="true">
      <variant returnType="void" isPureVirtual="false">
        <arg type="bool" typeName="bool" name="init"
        isConst="false" />
      </variant>
    </method>
    <method name="unloadPlugin" cppName="unloadPlugin"
    specifier="public" isStatic="true" isVirtual="false"
    isScriptOverwritable="true">
      <variant returnType="void" isPureVirtual="false">
        <arg type="QString &amp;" typeName="QString"
        name="fileName" isConst="true" />
        <arg type="bool" typeName="bool" name="remove"
        hasDefault="true" default=" false" isConst="false" />
      </variant>
    </method>
    <method name="postInitPlugins" cppName="postInitPlugins"
    specifier="public" isStatic="true" isVirtual="false"
    isScriptOverwritable="true">
      <variant returnType="void" isPureVirtual="false" />
    </method>
    <method name="postInitPlugin" cppName="postInitPlugin"
    specifier="public" isStatic="true" isVirtual="false"
    isScriptOverwritable="true">
      <variant returnType="void" isPureVirtual="false">
        <arg type="QObject *" typeName="QObject *" name="plugin"
        isConst="false" />
      </variant>
    </method>
    <method name="countPlugins" cppName="countPlugins"
    specifier="public" isStatic="true" isVirtual="false"
    isScriptOverwritable="true">
      <variant returnType="int" isPureVirtual="false" />
    </method>
    <method name="getPluginInfo" cppName="getPluginInfo"
    specifier="public" isStatic="true" isVirtual="false"
    isScriptOverwritable="true">
      <variant returnType="RPluginInfo" isPureVirtual="false">
        <arg type="int" typeName="int" name="i" isConst="false" />
      </variant>
    </method>
    <method name="getPluginsPath" cppName="getPluginsPath"
    specifier="public" isStatic="true" isVirtual="false"
    isScriptOverwritable="true">
      <variant returnType="QString" isPureVirtual="false" />
    </method>
  </class>
</unit>
